{
  "name": "react-immutable-treeview",
  "version": "0.2.0",
  "description": "React Tree View Component Accept Immutable.js Data",
  "main": "index.js",
  "scripts": {
    "example": "node ./example/devServer.js",
    "build": "npm run build:babel && npm run build:copy-files && npm run build:es",
    "build:es": " cross-env BABEL_ENV=es babel ./src/index.js --out-file ./build/index.es.js",
    "build:babel": "cross-env NODE_ENV=release babel ./src --out-dir ./build --ignore spec.js",
    "build:copy-files": "babel-node ./scripts/copy-files.js",
    "build:example": "npm run clean:build &&webpack --colors --config ./example/webpack.config.prod.js",
    "clean:build": "rimraf build",
    "prebuild": "npm run clean:build",
    "postpublish": "npm run build && cd build && npm publish",
    "test": "karma start karma.conf.js",
    "test-travis": "karma start --browsers Firefox --single-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hapood/react-immutable-treeview.git"
  },
  "keywords": [
    "react",
    "treeview",
    "data-driven",
    "immutable",
    "fast",
    "animation"
  ],
  "author": "Hapood Wang",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/hapood/react-immutable-treeview/issues"
  },
  "homepage": "https://github.com/hapood/react-immutable-treeview#readme",
  "peerDependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "dependencies": {
    "immutable": "^3.8.1",
    "prop-types": "^15.5.10",
    "react-immutable-proptypes": "^2.1.0",
    "velocity-react": "^1.1.4"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "chai": "^4.1.0",
    "cross-env": "^5.0.1",
    "enzyme": "^2.9.1",
    "fs-extra": "^4.0.0",
    "html-webpack-plugin": "^2.29.0",
    "immutable": "^3.8.1",
    "istanbul-instrumenter-loader": "^3.0.0",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-verbose-reporter": "^0.0.6",
    "karma-webpack": "^2.0.4",
    "loglevel": "^1.4.1",
    "mocha": "^3.4.2",
    "react": "^15.6.1",
    "react-addons-test-utils": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^1.3.1",
    "react-router-dom": "^4.1.2",
    "react-test-renderer": "^15.6.1",
    "rimraf": "^2.6.1",
    "sinon": "^2.4.1",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.5.1"
  }
}